{
  "name": "vite-plugin-i18next-checker",
  "version": "1.0.0",
  "description": "A Vite plugin for checking i18next translations",
  "main": "output/index.js",
  "module": "output/index.mjs",
  "types": "output/index.d.ts",
  "exports": {
    ".": {
      "import": "./output/index.mjs",
      "require": "./output/index.js",
      "types": "./output/index.d.ts"
    }
  },
  "files": [
    "output"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint source --ext .ts",
    "lint:fix": "eslint source --ext .ts --fix",
    "format": "prettier --write 'source/**/*.ts'",
    "format:check": "prettier --check 'source/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "plugin",
    "i18next",
    "i18n",
    "translation",
    "checker"
  ],
  "author": "yinxulai",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yinxulai/vite-plugin-i18next-checker.git"
  },
  "bugs": {
    "url": "https://github.com/yinxulai/vite-plugin-i18next-checker/issues"
  },
  "homepage": "https://github.com/yinxulai/vite-plugin-i18next-checker#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}
